{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "config": [
        "src/config"
      ],
      "components/*": [
        "src/components/*"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "page": [
        "src/page"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "types/*": [
        "src/types/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    },
    "experimentalDecorators": true
  }
}
